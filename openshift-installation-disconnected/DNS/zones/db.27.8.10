;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	bynet.dev. admin.bynet.dev. (
			      5		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - 
	IN	NS	open-dns.bynet.dev.

; PTR records
10	IN	PTR	open-dns.bynet.dev.
11	IN	PTR	openshift-reg.bynet.dev.
12	IN	PTR	open-lb.bynet.dev.
13	IN	PTR	openshift-test-WEB.bynet.dev.
20	IN	PTR	control-plane-0.offopen.bynet.dev
21	IN	PTR	control-plane-1.offopen.bynet.dev
22	IN	PTR	control-plane-2.offopen.bynet.dev
23	IN	PTR	compute-0.offopen.bynet.dev
24	IN	PTR	compute-1.offopen.bynet.dev
25	IN	PTR	compute-2.offopen.bynet.dev
26	IN	PTR	bootstrap-0.offopen.bynet.dev
15	IN	PTR	registry.bynet.dev
