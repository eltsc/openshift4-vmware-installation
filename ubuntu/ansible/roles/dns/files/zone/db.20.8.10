;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	bynet.dev. admin.bynet.dev. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - 
	IN	NS	openshift-test-DNS.bynet.dev.

; PTR records
113	IN	PTR	openshift-test-DNS.bynet.dev.
114	IN	PTR	openshift-test-DHCP.bynet.dev.
